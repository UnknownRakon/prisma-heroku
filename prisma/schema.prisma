generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id         String    @id @default(uuid())
  createdAt  DateTime  @default(now())
  updatedAt  DateTime? @updatedAt
  group      String
  level      String
  year       Int
  speciality String
  dorm       String
  stud       String?
  prof       Int?
  dormnum    Int?
}